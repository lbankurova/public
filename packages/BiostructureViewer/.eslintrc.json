{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"google"
	],
	"parserOptions": {
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"rules": {
		"indent": [
			"error",
			2
		],
		"max-len": [
			"error",
			120
		],
		"require-jsdoc": "off",
		"spaced-comment": "off"
	},
	"parser": "@typescript-eslint/parser",
	"plugins": [
		"@typescript-eslint"
	]
}