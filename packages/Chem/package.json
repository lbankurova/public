{
  "name": "@datagrok/chem",
  "friendlyName": "Chem",
  "fullName": "Chem",
  "version": "0.50.1",
  "description": "Cheminformatics support: \n\n* RDKit-based rendering \n* Molecule sketching \n*",
  "properties": [
    {
      "name": "Renderer",
      "propertyType": "string",
      "choices": [
        "RDKit",
        "OpenChemLib"
      ],
      "defaultValue": "RDKit",
      "nullable": false
    }
  ],
  "sources": [
    "common/ngl_viewer/ngl.js"
  ],
  "dependencies": {
    "datagrok-api": "^0.123.0",
    "@datagrok-libraries/ml": "^0.0.14",
    "@datagrok-libraries/utils": "^0.1.0",
    "@types/wu": "latest",
    "cash-dom": "latest",
    "css-loader": "latest",
    "dayjs": "latest",
    "file-loader": "latest",
    "offscreen-canvas": "latest",
    "openchemlib": "6.0.1",
    "path": "^0.12.7",
    "rxjs": "^6.5.5",
    "style-loader": "latest",
    "ts-loader": "latest",
    "typescript": "latest",
    "worker-loader": "latest",
    "wu": "latest"
  },
  "devDependencies": {
    "@types/offscreencanvas": "latest",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.1.0",
    "eslint-config-google": "^0.14.0",
    "path": "^0.12.7",
    "webpack": "latest",
    "webpack-cli": "latest",
    "workerloader-jest-transformer": "^0.0.5"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "crypto": false
  },
  "scripts": {
    "link-api": "npm install && npm link datagrok-api",
    "link-utils": "npm install && npm link @datagrok-libraries/utils",
    "link-ml": "npm install && npm link @datagrok-libraries/ml",
    "link-all": "npm install && npm link datagrok-api @datagrok-libraries/utils @datagrok-libraries/ml",
    "build-chem-local": "npm --prefix ./../../js-api run build && npm --prefix ./../../libraries/utils run build && npm --prefix ./../../libraries/ml run build && npm run build",
    "build": "webpack",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint-fix": "eslint \"./src/**/*.ts\" --fix",
    "test": "set HOST=dev && jest"
  },
  "canEdit": [
    "Developers"
  ],
  "canView": [
    "All users"
  ]
}
